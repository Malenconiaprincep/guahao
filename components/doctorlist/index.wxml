<template name="doctorlist">
  <block wx:for="{{list}}" wx:key="unique">
    <view class="doctorlist flex flex-auto">
      <view class="list_l">
        <image src="{{item.pic}}" class="avatar"></image>
      </view>
      <view class="list_r">
        <view class="name">
          <text class="name">{{item.name}}</text>
        </view>
        <view class="desc">
          <text>{{item.desc}}</text>
        </view>
      </view>
    </view>
  </block>
</template>
